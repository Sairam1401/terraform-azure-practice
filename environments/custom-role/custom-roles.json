{
    "custom_roles": [
        {
            "rolename": "CustomAzureAutoScalingAdministrator",
            "description": "Can Manage autoscaling settings for Azure Resources",
            "type": "CustomRole",
            "permissions": [
                {
                    "actions": [
                        "Microsoft.Insights/autoscaleSettings/*",
                        "Microsoft.Insights/diagnosticSettings/*",
                        "Microsoft.Resources/deployments/*",
                        "Microsoft.Resources/subscriptions/resourceGroups/deployments/*",
                        "Microsoft.Support/*"
                    ],
                    "notActions": [],
                    "dataActions": [
                        "*"
                    ],
                    "notDataActions": []
                }
            ]
        },
        {
            "rolename": "CustomAzureAccessReviewer",
            "description": "Can review permissions, resource access and generate reports",
            "type": "CustomRole",
            "permissions": [
                {
                    "actions": [
                        "Microsoft.Authorization/*/read",
                        "Microsoft.Resources/subscriptions/resourceGroups/read",
                        "Microsoft.Support/*"
                    ],
                    "notActions": [
                        "Microsoft.Authorization/*/write",
                        "Microsoft.Authorization/*/delete"
                    ],
                    "dataActions": [
                        "*"
                    ],
                    "notDataActions": []
                }
            ]
        }
    ]
}